// GENERAL DEBUG TEST
// Quick test for debugging various features

func main[] |
  print["=== DEBUG TEST START ==="]
  
  // Test 1: Basic variables
  let a := 10
  let b := 20
  print["a = " + str[a] + ", b = " + str[b]]
  
  // Test 2: Expression evaluation
  let c := a + b
  print["a + b = " + str[c]]
  
  // Test 3: Function definition and call
  func double[n] |
    return[n * 2]
  ^
  
  let doubled := double[a]
  print["double(a) = " + str[doubled]]
  
  // Test 4: Array access
  let arr := [1, 2, 3]
  print["arr[1] = " + str[arr[1]]]
  
  // Test 5: Dictionary access
  let dict := {"test": 123}
  print["dict['test'] = " + str[dict["test"]]]
  
  // Test 6: Conditional
  if [a < b] |
    print["a < b is true"]
  ^ else |
    print["a < b is false"]
  ^
  
  // Test 7: Loop
  let i := 0
  while [i < 3] |
    print["Loop: i = " + str[i]]
    i := i + 1
  ^
  
  // Test 8: Nested structures
  let nested := [{"value": 42}]
  print["nested[0]['value'] = " + str[nested[0]["value"]]]
  
  print["=== DEBUG TEST END ==="]
^

main[]