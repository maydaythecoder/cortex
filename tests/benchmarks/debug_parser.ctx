// PARSER DEBUG TEST
// Minimal test case for debugging parser issues

func main[] |
  print["Parser debug test"]
  
  // Test variable declaration
  let x := 42
  print["x = " + str[x]]
  
  // Test arithmetic
  let y := x + 10
  print["y = " + str[y]]
  
  // Test function call
  func test[] |
    return[100]
  ^
  
  let result := test[]
  print["result = " + str[result]]
  
  // Test array
  let arr := [1, 2, 3]
  print["arr = " + str[arr]]
  
  // Test dictionary
  let dict := {"key": "value"}
  print["dict = " + str[dict]]
  
  // Test conditional
  if [x > 0] |
    print["x is positive"]
  ^
  
  // Test loop
  let i := 0
  while [i < 3] |
    print["i = " + str[i]]
    i := i + 1
  ^
  
  print["Parser debug complete"]
^

main[]