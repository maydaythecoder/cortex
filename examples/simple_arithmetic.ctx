// Simple arithmetic operations example
// Demonstrates basic arithmetic operations without string concatenation
//FUNCTIONAL
func main[] |
  // Basic arithmetic
  let a := 10
  let b := 3
  
  let sum := a + b
  let diff := a - b
  let product := a * b
  let quotient := a / b
  
  // Print results as numbers only
  print[sum]
  print[diff]
  print[product]
  print[quotient]
^

main[]
